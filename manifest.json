{
  "manifest_version": 2,
  "name": "Startpage search",
  "short_name": "st_search",
  "description": "Adds Startpage search as a search engine.",
  "version": "1.1.0",
  "icons": {
    "64": "icons/sp-icon.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Startpage search",
      "keyword": "@startpage",
      "search_url": "https://www.startpage.com/do/dsearch?query={searchTerms}&cat=web",
      "suggest_url": "https://www.startpage.com/suggestions?q={searchTerms}&format=opensearch",
      "favicon_url": "https://www.startpage.com/favicon.ico"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "StartpageSearchExtension@roteKlaue",
      "strict_min_version": "64.0"
    }
  },
  "author": "roteKlaue",
  "homepage_url": "https://github.com/roteKlaue/firefox-startpage-search-extension"
}
